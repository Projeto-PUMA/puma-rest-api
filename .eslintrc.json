{
    "extends": "airbnb",
    "rules": {
        "semi": 2,
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true
            }
        ],
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js"
                ]
            }
        },
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "next"
            }
        ]
    }
}